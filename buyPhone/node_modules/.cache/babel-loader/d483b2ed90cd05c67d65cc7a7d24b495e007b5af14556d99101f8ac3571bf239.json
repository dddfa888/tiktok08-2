 <div class=\"timeline\">\r\n      <div class=\"timeline-item\" v-for=\"(item, index) in newsList\" :key=\"index\">\r\n        <div class=\"circle\"></div>\r\n        <div class=\"timeline-content\">\r\n          <div class=\"date\">{{ item.date }}</div>\r\n          <div class=\"title\">{{ item.title }}</div>\r\n          <div class=\"desc\">{{ item.desc }}</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 底部横幅 -->\r\n    <div\r\n      class=\"join-us\"\r\n      :style=\"{\r\n        backgroundImage: `url('${bannar2}')`,\r\n        backgroundRepeat: 'no-repeat',\r\n        backgroundPosition: 'center',\r\n        backgroundSize: 'cover'\r\n      }\"\r\n    >\r\n      <div class=\"join-text\">加入我们</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport bannar1 from \"@/assets/image/bannar.jpg\";\r\nimport bannar2 from \"@/assets/image/bannar2.jpg\";\r\nimport bannar3 from \"@/assets/image/bannar3.jpg\";\r\nimport bannar5 from \"@/assets/image/bannar5.jpg\";\r\nimport backImg from \"@/assets/image/back.jpg\";\r\nimport nav1 from \"@/assets/image/nav1.png\";\r\nimport nav2 from \"@/assets/image/nav2.png\";\r\nimport nav3 from \"@/assets/image/nav3.png\";\r\nimport nav4 from \"@/assets/image/nav4.png\";\r\nimport nav5 from \"@/assets/image/nav5.png\";\r\nimport nav6 from \"@/assets/image/nav6.png\";\r\nimport nav7 from \"@/assets/image/nav7.png\";\r\nimport nav8 from \"@/assets/image/nav8.png\";\r\nimport nav9 from \"@/assets/image/nav9.png\";\r\nimport nav10 from \"@/assets/image/nav10.png\";\r\nimport nav11 from \"@/assets/image/nav11.png\";\r\nimport nav12 from \"@/assets/image/nav12.png\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      backImg,\r\n      bannar2,\r\n      slides: [bannar1, bannar2, bannar3, bannar5],\r\n      navList1: [\r\n        { name: \"SPAR中国2024\", img: nav1 },\r\n        { name: \"402亿元销售额\", img: nav2 },\r\n        { name: \"全国41个城市\", img: nav3 },\r\n        { name: \"1140家门店\", img: nav4 },\r\n        { name: \"15座配送中心\", img: nav5 },\r\n        { name: \"50,000名员工\", img: nav6 }\r\n      ],\r\n      navList2: [\r\n        { name: \"SPAR国际2024\", img: nav7 },\r\n        { name: \"493亿欧元销售额\", img: nav8 },\r\n        { name: \"全球49个国家\", img: nav9 },\r\n        { name: \"13,809家门店\", img: nav10 },\r\n        { name: \"258座配送中心\", img: nav11 },\r\n        { name: \"470,000名员工\", img: nav12 }\r\n      ],\r\n      newsList: [\r\n        {\r\n          date: \"09-29 2025\",\r\n          title: \"重装开业 | SPAR家家悦菏泽龙广店重装启幕\",\r\n          desc: \"SPAR家家悦菏泽龙广店盛装启幕，为顾客带来更舒适的购物体验。\"\r\n        },\r\n        {\r\n          date: \"09-22 2025\",\r\n          title: \"研学课堂 | 跟SPAR“小店长”一起探索超市的秘密\",\r\n          desc: \"SPAR组织儿童体验营活动，让孩子们走进超市学习知识、体验职业。\"\r\n        },\r\n        {\r\n          date: \"09-15 2025\",\r\n          title: \"重装开业 | SPAR家家悦济南历城新店启幕\",\r\n          desc: \"SPAR历城新店正式开业，提供优质商品和舒适购物环境。\"\r\n        }\r\n      ]\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.app {\r\n  width: 100%;\r\n  overflow-x: hidden;\r\n  font-family: \"PingFang SC\", sans-serif;\r\n  padding-bottom: 50px;\r\n}\r\n\r\n/* 顶部导航 */\r\nheader {\r\n  height: 50px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: #fff;\r\n  position: fixed;\r\n  top: 0;\r\n  width: 100%;\r\n  z-index: 99;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.05);\r\n}\r\n.logo {\r\n  width: 120px;\r\n  height: auto;\r\n}\r\n\r\n/* 轮播图 */\r\n.carousel {\r\n  margin-top: 50px;\r\n}\r\n.carousel-img {\r\n  width: 100%;\r\n  height: 220px;\r\n  object-fit: cover;\r\n}\r\n\r\n/* nav 区域 */\r\n.nav-section {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  background: #f5f5f5;\r\n  padding: 20px 0;\r\n  justify-content: space-around;\r\n}\r\n.nav-item {\r\n  width: 30%;\r\n  text-align: center;\r\n  margin-bottom: 15px;\r\n}\r\n.nav-item img {\r\n  width: 50px;\r\n  height: 50px;\r\n  margin-bottom: 8px;\r\n}\r\n.nav-text {\r\n  font-size: 13px;\r\n  font-weight: 500;\r\n}\r\n.nav-text.red {\r\n  color: #dd3333;\r\n}\r\n.nav-text.green {\r\n  color: #2c8750;\r\n}\r\n\r\n/* 背景模块 */\r\n.back {\r\n  width: 100%;\r\n  height: 220px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: white;\r\n  font-size: 18px;\r\n  background-size: cover;\r\n  text-align: center;\r\n}\r\n\r\n/* 时间轴 */\r\n.timeline {\r\n  background: #fff;\r\n  padding: 20px;\r\n}\r\n.timeline-item {\r\n  position: relative;\r\n  padding-left: 30px;\r\n  margin-bottom: 25px;\r\n}\r\n.circle {\r\n  width: 12px;\r\n  height: 12px;\r\n  background: #0f6532;\r\n  border-radius: 50%;\r\n  position: absolute;\r\n  left: 0;\r\n  top: 10px;\r\n}\r\n.timeline-content .date {\r\n  font-size: 12px;\r\n  color: #888;\r\n  margin-bottom: 5px;\r\n}\r\n.timeline-content .title {\r\n  font-size: 15px;\r\n  font-weight: 600;\r\n  color: #222;\r\n  margin-bottom: 4px;\r\n}\r\n.timeline-content .desc {\r\n  font-size: 13px;\r\n  color: #555;\r\n  line-height: 1.4;\r\n}\r\n\r\n/* 底部横幅 */\r\n.join-us {\r\n  width: 100%;\r\n  height: 200px;\r\n  position: relative;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: white;\r\n  font-size: 18px;\r\n}\r\n.join-us::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  inset: 0;\r\n  background-color: rgba(0, 0, 0, 0.35);\r\n}\r\n.join-text {\r\n  position: relative;\r\n  z-index: 2;\r\n}\r\n</style>\r\n"],"mappings":"AAyEA,OAAAA,OAAA;AACA,OAAAC,OAAA;AACA,OAAAC,OAAA;AACA,OAAAC,OAAA;AACA,OAAAC,OAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,IAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;IACA;MACAb,OAAA;MACAH,OAAA;MACAiB,MAAA,GAAAlB,OAAA,EAAAC,OAAA,EAAAC,OAAA,EAAAC,OAAA;MACAgB,QAAA,GACA;QAAAC,IAAA;QAAAC,GAAA,EAAAhB;MAAA,GACA;QAAAe,IAAA;QAAAC,GAAA,EAAAf;MAAA,GACA;QAAAc,IAAA;QAAAC,GAAA,EAAAd;MAAA,GACA;QAAAa,IAAA;QAAAC,GAAA,EAAAb;MAAA,GACA