og\r\n      class=\"es-dialog\"\r\n      :visible.sync=\"dialogVisible\"\r\n      :center=\"true\"\r\n      :destroy-on-close=\"true\"\r\n    >\r\n      <div slot=\"title\" class=\"dialog-title\">\r\n        <span>{{ $t(\"message.home.addCart\" /** 添加购物车*/) }}</span>\r\n      </div>\r\n      <div class=\"dialog-content\" v-if=\"dialogVisible\">\r\n        <EsProductInfo :id=\"currentId\" @handleBuy=\"handleBuy\" />\r\n      </div>\r\n      <span slot=\"footer\"></span>\r\n    </el-dialog>\r\n\r\n    <div class=\"page-title\" style=\"cursor: pointer\" @click=\"goBack\">\r\n      <i class=\"el-icon-arrow-left\"></i>\r\n      {{ $t(\"message.home.myCommodityCollection\") }}\r\n    </div>\r\n    <div v-loading=\"loading\" >\r\n      <div ref=\"collentGoods\" class=\"content\">\r\n        <div v-if=\"listData.length\" class=\"list-content\">\r\n        <div v-for=\"item in listData\" :key=\"item.sellerId\" class=\"item\" >\r\n          <div class=\"discount\" v-if=\"item.discountRatio\">\r\n            <span>{{ numberFormatFn(item.discountRatio) * 100 }} %</span>\r\n            <br>\r\n            <span>OFF</span>\r\n          </div>\r\n          <div class=\"poster\" @click=\"gotoDetails(item)\">\r\n            <img\r\n              :src=\"\r\n                item.imgUrl1 || item.imgUrl2 || item.imgUrl3 || item.imgUrl4\r\n              \"\r\n              alt=\"collection-item\"\r\n            />\r\n          </div>\r\n          <div class=\"price\">\r\n            <p>\r\n              ${{ numberFormatFn(item.discountPrice || item.sellingPrice) }}\r\n            </p>\r\n            <i class=\"el-icon-star-on\" @click=\"cancelCol(item)\" />\r\n          </div>\r\n          <div class=\"name\">\r\n            <p @click=\"gotoDetails(item)\">{{ item.name || \"-\" }}</p>\r\n            <div>\r\n              <i class=\"el-icon-shopping-cart-full\"></i>\r\n              <span class=\"buy-btn\" @click=\"buyNow(item)\">\r\n                {{ $t(\"message.home.buyNow\" /**立即购买 */) }}\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      </div>\r\n      \r\n      <el-empty\r\n        v-if=\"!listData.length && !loading\"\r\n        :description=\"$t('message.home.noData')\"\r\n      ></el-empty>\r\n    </div>\r\n    <div v-if=\"listData.length || total\" class=\"common-pagination\">\r\n      <el-pagination\r\n        background\r\n        class=\"es-pagination\"\r\n        layout=\"prev,pager, next\"\r\n        :pager-count=\"5\"\r\n        :page-size=\"pageSize\"\r\n        :current-page=\"pageNum\"\r\n        :total=\"total\"\r\n        @current-change=\"currentChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import EsProductInfo from \"@/components/productInfo\";\r\n  import { KeepGoodsListApi, KeepGoodsCancelApi } from \"@/api/userCenter\";\r\n  import { numberFormat } from \"@/util\";\r\n  export default {\r\n    name: \"CollectGoods\",\r\n    components: { EsProductInfo },\r\n    data() {\r\n      return {\r\n        listData: [],\r\n        loading: true,\r\n        pageNum: 1,\r\n        pageSize: 8,\r\n        total: 0,\r\n        dialogVisible: false,\r\n        currentId: \"\",\r\n      };\r\n    },\r\n    mounted() {\r\n      this.$nextTick(() => {\r\n        this.getListData();\r\n      });\r\n      this.$store.state.user.currentIndex = 3;\r\n    },\r\n    methods: {\r\n      goBack() {\r\n        this.$router.go(-1);\r\n      },\r\n\r\n      numberFormatFn(num) {\r\n        return numberFormat(num);\r\n      },\r\n\r\n      currentChange(page) {\r\n        this.pageNum = page;\r\n        this.getListData();\r\n      },\r\n\r\n      getListData() {\r\n        this.loading = true;\r\n        const params = {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        };\r\n        KeepGoodsListApi(params)\r\n          .then((res) => {\r\n            const data = res.data;\r\n            // console.log(\"this.listData ->\", data);\r\n            this.total = res.data.pageInfo?.totalElements || 0;\r\n            data.pageList.forEach((item) => {\r\n              item.loading = false;\r\n            });\r\n            this.listData = data.pageList;\r\n            // console.log(\" this.listData->\", this.total);\r\n            // collentGoods\r\n            this.$Gsap.from(this.$refs.collentGoods,{\r\n                delay: 0.5,\r\n                duration: 1,\r\n                y: '+100',\r\n                autoAlpha: 0,\r\n                ease: \"back.out(1.7)\"\r\n              })\r\n            this.loading = false;\r\n          })\r\n          .catch(() => {\r\n            this.loading = false;\r\n          });\r\n      },\r\n\r\n      cancelCol(data) {\r\n        KeepGoodsCancelApi({ sellerGoodsId: data.id }).then(() => {\r\n          this.pageNum = 1;\r\n          this.getListData();\r\n        });\r\n      },\r\n\r\n      buyNow(data) {\r\n        this.currentId = data.id;\r\n        setTimeout(() => {\r\n          this.dialogVisible = true;\r\n        }, 300);\r\n      },\r\n\r\n      handleBuy() {\r\n        this.dialogVisible = false;\r\n      },\r\n\r\n      gotoDetails(data) {\r\n        if (data.isShelf) {\r\n        this.$router.push({ name: \"productDetails\", query: { id: data.id } });\r\n      } else {\r\n        this.$message({\r\n          type: \"warning\",\r\n          message: this.$t(\"message.home.productAvailable\"),\r\n        });\r\n      }\r\n        // this.$router.push({\r\n        //   name: \"productDetails\",\r\n        //   query: { id: data.id },\r\n        // });\r\n      },\r\n    },\r\n  };\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\nhtml[dir=\"rtl\"] {\r\n  .collect-content{\r\n    padding-left: 0;\r\n    padding-right: 50px;\r\n  }\r\n}\r\n  .collect-content {\r\n    padding-top: 32px;\r\n    padding-left: 50px;\r\n     .content {\r\n      margin-top: 15px;\r\n      min-height: 120px;\r\n      > .list-content {\r\n        overflow: hidden;\r\n        > .item {\r\n          width: 200px;\r\n          border: 1px solid #eeeeee;\r\n          border-radius: 4px;\r\n          float: left;\r\n          margin-right: calc(52px / 3);\r\n          margin-bottom: 20px;\r\n          overflow: hidden;\r\n          position: relative;\r\n          .discount{\r\n            width: 31px;\r\n            height: 32px;\r\n            background: url(\"../../../assets/image/discount.png\") no-repeat 100%/100%;\r\n            position: absolute;\r\n            top: 0;\r\n            right: 0;\r\n            text-align: center;\r\n            font-size: 11px;\r\n            color: #fff;\r\n            font-weight: 500;\r\n          }\r\n          &:nth-child(4n) {\r\n            margin-right: 0;\r\n          }\r\n          > .poster {\r\n            width: 180px;\r\n            height: 180px;\r\n            padding: 10px;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: center;\r\n            overflow: hidden;\r\n            cursor: pointer;\r\n            > img {\r\n              width: 100%;\r\n              height: auto;\r\n            }\r\n          }\r\n          > .price {\r\n            padding: 0 10px;\r\n            display: flex;\r\n            align-items: center;\r\n            justify-content: space-between;\r\n            margin-top: 10px;\r\n            > p {\r\n              font-size: 16px;\r\n              color: var(--color-price);\r\n            }\r\n            > i {\r\n              font-size: 18px;\r\n              color: var(--color-main);\r\n              cursor: pointer;\r\n            }\r\n          }\r\n          > .name {\r\n            padding: 10px;\r\n            height: 90px;\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: space-between;\r\n            cursor: pointer;\r\n            > p {\r\n              overflow: hidden;\r\n              text-overflow: ellipsis;\r\n              display: -webkit-box;\r\n              -webkit-line-clamp: 2;\r\n              -webkit-box-orient: vertical;\r\n            }\r\n            > div {\r\n              cursor: pointer;\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n</style>\r\n"],"mappings":";;;AA8EA,OAAAA,aAAA;AACA,SAAAC,gBAAA,EAAAC,kBAAA;AACA,SAAAC,YAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAL;EAAA;EACAM,KAAA;IACA;MACAC,QAAA;MACAC,OAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,aAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;MACA,KAAAC,WAAA;IACA;IACA,KAAAC,MAAA,CAAAC,KAAA,CAAAC,IAAA,CAAAC,YAAA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,EAAA;IACA;IAEAC,eAAAC,GAAA;MACA,OAAAvB,YAAA,CAAAuB,GAAA;IACA;IAEAC,cAAAC,IAAA;MACA,KAAAnB,OAAA,GAAAmB,IAAA;MACA,KAAAZ,WAAA;IACA;IAEAA,YAAA;MACA,KAAAR,OAAA;MACA,MAAAqB,MAAA;QACApB,OAAA,OAAAA,OAAA;QACAC,QAAA,OAAAA;MACA;MACAT,gBAAA,CAAA4B,MAAA,EACAC,IAAA,CAAAC,GAAA;QAAA,IAAAC,kBAAA;QACA,MAAA1B,IAAA,GAAAyB,GAAA,CAAAzB,IAAA;QACA;QACA,KAAAK,KAAA,KAAAqB,kBAAA,GAAAD,GAAA,CAAAzB,IAAA,CAAA2B,QAAA,cAAAD,kBAAA,uBAAAA,kBAAA,CAAAE,aAAA;QACA5B,IAAA,CAAA6B,QAAA,CAAAC,OAAA,CAAAC,IAAA;UACAA,IAAA,CAAA7B,OAAA;QACA;QACA,KAAAD,QAAA,GAAAD,IAAA,CAAA6B,QAAA;QACA;QACA;QACA,KAAAG,KAAA,CAAAC,IAAA,MAAAC,KAAA,CAAAC,YAAA;UACAC,KAAA;UACAC,QAAA;UACAC,CAAA;UACAC,SAAA;UACAC,IAAA;QACA;QACA,KAAAtC,OAAA;MACA,GACAuC,KAAA;QACA,KAAAvC,OAAA;MACA;IACA;IAEAwC,UAAA1C,IAAA;MACAJ,kBAAA;QAAA+C,aAAA,EAAA3C,IAAA,CAAA4C;MAAA,GAAApB,IAAA;QACA,KAAArB,OAAA;QACA,KAAAO,WAAA;MACA;IACA;IAEAmC,OAAA7C,IAAA;MACA,KAAAO,SAAA,GAAAP,IAAA,CAAA4C,EAAA;MACAE,UAAA;QACA,KAAAxC,aAAA;MACA;IACA;IAEAyC,UAAA;MACA,KAAAzC,aAAA;IACA;IAEA0C,YAAAhD,IAAA;MACA,IAAAA,IAAA,CAAAiD,OAAA;QACA,KAAAhC,OAAA,CAAAiC,IAAA;UAAApD,IAAA;UAAAqD,KAAA;YAAAP,EAAA,EAAA5C,IAAA,CAAA4C;UAAA;QAAA;MACA;QACA,KAAAQ,QAAA;UACAC,IAAA;UACAC,OAAA,OAAAC,EAAA;QACA;MACA;MACA;MACA;MACA;MACA;IACA;EACA;AACA","ignoreList":[]}]}