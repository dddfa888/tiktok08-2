ess(item) {\r\n      console.log('选择地址', item)\r\n      this.updatePaySelectAddress(item)\r\n      this.dialogVisible = false\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.item {\r\n  width: 100%;\r\n  border: 1px solid var(--color-border);\r\n  padding: 12px 16px;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  margin-bottom: 20px;\r\n  &:hover {\r\n    border-color: var(--color-main);\r\n  }\r\n  .info {\r\n    margin-bottom: 8px;\r\n  }\r\n}\r\n\r\n.address-active {\r\n  border-color: var(--color-main);\r\n}\r\n</style>\r\n"]}]}